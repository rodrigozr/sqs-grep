  "author": "Rodrigo Rosauro",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.509.0",
    "chalk": "^2.4.2",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "password-prompt": "^1.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^6.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "pkg": "^4.4.0",
    "sinon": "^7.5.0",
    "snyk": "^1.231.1"
  }
